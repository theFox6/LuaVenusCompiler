function callit(fun,t1,t2)
	return fun(t1,t2)
end

assert(
	callit(() => {
		return "testing"
	})
	== "testing")

assert(
	callit((k,v) => {
		return k.." = "..v
	}, "this test", "more test")
	== "this test = more test"
)
	
assert(
	callit((a , b) => {
		return (a-b)*4
	}, 10, 6) == 16
)

